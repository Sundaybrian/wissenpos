{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "User",
    "description": "The User",
    "required": [
        "firstName",
        "lastName",
        "phoneNumber",
        "email",
        "password",
        "role"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the user."
        },
        "firstName": {
            "$id": "#/properties/firstName",
            "type": "string",
            "title": "The firstName of the user."
        },
        "lastName": {
            "$id": "#/properties/lastName",
            "type": "string",
            "title": "The lastName of the user."
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email of the user."
        },
        "phoneNumber": {
            "$id": "#/properties/phoneNumber",
            "type": "string",
            "title": "The phoneNumber of the user."
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password of the user."
        },
        "image_url": {
            "$id": "#/properties/image_url",
            "type": "string",
            "title": "The image_url of the user."
        },
        "role": {
            "$id": "#/properties/role",
            "type": "string",
            "title": "The role of the user."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the user was deleted."
        }
    }
}
