{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Account",
    "description": "The Account",
    "required": ["company_id", "staff_id"],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the Company."
        },
        "company_id": {
            "$id": "#/properties/company_id",
            "type": "integer",
            "title": "The company_id of the Company."
        },
        "staff_id": {
            "$id": "#/properties/staff_id",
            "type": "integer",
            "title": "The staff_id of the user."
        },

        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the company was deleted."
        }
    }
}
