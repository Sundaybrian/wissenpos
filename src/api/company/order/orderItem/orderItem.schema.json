{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Order",
    "description": "The Order mOdel json schema",
    "required": ["cart_id", "product_id", "quantity"],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the Order."
        },
        "cart_id": {
            "$id": "#/properties/cart_id",
            "type": "string",
            "title": "The cart_id of the Order."
        },
        "quantity": {
            "$id": "#/properties/quantity",
            "type": "integer",
            "title": "The database quantity of the item ."
        },
        "order_status": {
            "$id": "#/properties/order_status",
            "type": "string",
            "title": "The order_status of the Order."
        },
        "company_id": {
            "$id": "#/properties/company_id",
            "type": "integer",
            "title": "The company_id of the Order."
        },
        "purchase_status": {
            "$id": "#/properties/purchase_status",
            "title": "The Order purchase_status.",
            "type": "string",
            "examples": ["paid", "unpaid"]
        },
        "product_id": {
            "$id": "#/properties/product_id",
            "type": "integer",
            "title": "The product_id of the Order."
        },
        "subtotal": {
            "$id": "#/properties/subtotal",
            "title": "The Order subtotal.",
            "type": "number",
            "examples": ["paid", "unpaid"]
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the Order was deleted."
        }
    }
}
