{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Company",
    "description": "The Company",
    "required": ["name", "description", "owner"],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the Company."
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of the Company."
        },
        "logo_url": {
            "$id": "#/properties/logo_url",
            "type": "string",
            "title": "The logo_url of the Company."
        },
        "website_url": {
            "$id": "#/properties/website_url",
            "type": "string",
            "title": "The website_url of the Company."
        },
        "email": {
            "$id": "#/properties/email",
            "title": "The company email.",
            "examples": ["sunday@omwami.com"]
        },
        "description": {
            "$id": "#/properties/description",
            "type": "string",
            "title": "The description of the Company.",
            "examples": ["some short desc of the action taken by the"]
        },

        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the company was deleted."
        }
    }
}
