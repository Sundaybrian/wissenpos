{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Category",
    "description": "The Category",
    "required": ["name", "menu_id"],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the Category."
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of the Category."
        },
        "menu_id": {
            "$id": "#/properties/menu_id",
            "type": "integer",
            "title": "The menu_id the Category belongs to."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date when the ticket was created."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date when the ticket was updated."
        },
        "deleted_at": {
            "$id": "#/properties/deleted_at",
            "type": "string",
            "title": "The date when the Category was deleted."
        }
    }
}
