        "joi": "^17.2.1",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.21.5-next2",
        "morgan": "^1.10.0",
        "nodemailer": "^6.4.14",
        "objection": "^2.2.3",
        "pg": "^8.3.0",
        "rootpath": "^0.1.2",
        "swagger-jsdoc": "^6.0.0-rc.3",
        "swagger-ui-express": "^4.1.4",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "apidoc": "^0.24.0",
        "apidoc-swagger": "^0.3.2",
        "apidoc-swagger-3": "^1.0.6",
        "eslint": "^7.7.0",
